&qupv3_se8_i2c {
	status = "ok";
	idtp9418: idtp9418@3b {
		compatible = "idt,p9418";
		status = "ok";
		reg = <0x3b>;
		idt,irq = <&tlmm 113 0x00>;
		hall,int3 = <&tlmm 136 0x00>;
		hall,int4 = <&tlmm 138 0x00>;
		idt,reverse-enable = <&tlmm 47 0x00>;
		idt,reverse-booset-enable = <&tlmm 22 0x00>;
