		vib_mode = < 0 >;   /*mode:   RAM mode->0; CONT mode -> 1*/
		vib_f0_pre = < 1700 >;
		vib_f0_cali_percen = < 9 >;
		vib_cont_drv_lev = < 38 >;
		vib_cont_drv_lvl_ov = < 50 >;
		vib_cont_td = < 0x009A >;
		vib_cont_zc_thr = < 0x0FF1 >;  /*hq 0x08F8 */
		vib_cont_num_brk = < 3 >;
		vib_f0_coeff = < 260 >; /*Don't modify it    2.604167*/
